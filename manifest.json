{
  "manifest_version": 2,
  "name": "My Cool Extension",
  "version": "0.1",
"content_scripts": [
  {
    "matches": [
      "<all_urls>"
    ],
    "js": ["content.js"]
  }
],
"icons": { "16": "Itachi.png",
           "48": "Itachi.png",
          "128": "Itachi.png" },

"background": {
  "scripts": [
    "background.js"
  ]
},


"permissions": ["http://*.google.com/", "https://*.google.com/"],
  "browser_action": {
    "default_title": "",
    
    "default_popup": "Vinno.html"
  }

}